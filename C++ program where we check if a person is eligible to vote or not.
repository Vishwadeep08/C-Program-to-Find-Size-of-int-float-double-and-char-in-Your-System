#include<iostream>
using namespace std;

int main()
{
	int age;

	cout<<"Enter your age: ";
	cin>>age;

	if(age>=13 && age<=19)
	{
		cout<<"You're still a teenager"<<endl;
	}
	else
	{
		cout<<"You're not a Teenager"<<endl;
	}


	//condition to check voting eligility
	if(age>=18)
	{
		cout<<"You're eligible for voting"<<endl;
	}
	else
	{
		cout<<"You're not eligible for voting"<<endl;
	}

	return 0;
}
